{
  "name": "Node_Kernel",
  "version": "1.0.0",
  "description": "Node Js Kernel functionnal with Typescript and Sequelize",
  "main": "index.js",
  "author": "D Smart",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fabric:component": "ts-node bin/generator/component.ts",
    "fabric:migrate:token": "prisma migrate deploy --schema prisma/jwt/schema.prisma",
    "fabric:push:token": "prisma db push --schema prisma/jwt/schema.prisma",
    "fabric:migrate": "prisma migrate deploy",
    "fabric:push": "prisma db push",
    "start": "nodemon --watch \"./**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node index.ts\"\n",
    "tsoa": "tsoa spec-and-routes -c ./tsoa.json",
    "tsoa:watch": "nodemon --watch ./src/**/*.controller.ts --watch ./tsoa.json -e ts,json --exec npm run tsoa",
    "docker:build": "docker build --tag node-docker .",
    "docker:start": "docker run -it node-docker"
  },
  "keywords": [
    "Framework NodeJS",
    "NodeJS Kernel with Typescript",
    "Typescript Kernel"
  ],
  "license": "ISC",
  "dependencies": {
